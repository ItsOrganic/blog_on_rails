<%= form_with model: @blog_post do |form| %>
  <!-- Your form fields for title and body go here -->
  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="field">
    <%= form.label :body %>
    <%= form.text_area :body %>
  </div>
  <div class="actions">
    <%= form.submit %>
  </div>
  <div>
  <%= form.label :published_at %>
  <%= form.datetime_select :published_at, include_blank: true %>
  </div>
<% end %>